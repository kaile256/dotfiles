snippet vader
alias vim
  language: sh

  before_script: |
    git clone https://github.com/junegunn/vader.vim.git

  script: |
    vim -Nu <(cat << VIMRC
    filetype off
    set rtp+=vader.vim
    set rtp+=.
    ${2:set rtp+=after}
    filetype plugin indent on
    syntax enable
    VIMRC) -c 'Vader! test/*' >/dev/null
    )
