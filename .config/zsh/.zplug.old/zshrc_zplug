source $HOME/.zplug/init.zsh

zplug "zplug/zplug"
zplug "b4b4r07/enhanced", use:"init.sh"

# Frameworks
zplug "mollifier/anyframe" # provides wraper func inc., peco, fzf, percol, zaw

## Syntax
# 2: read after compinit
zplug "zsh-users/zsh-syntax-highlighting", defer:2
zplug "zsh-users/zsh-completions"


if ! zplug check --verbose; then
    printf "Install? [y/N]: "
    if read -q; then
        echo; zplug install
    fi
    echo
fi

zplug load

# The following lines were added by compinstall
zstyle :compinstall filename "${HOME}/.zshrc"

# End of lines added by compinstall
# Lines configured by zsh-newuser-install
HISTFILE=~/.histfile
HISTSIZE=1000
SAVEHIST=1000
setopt beep # when zle error
setopt extendedglob
setopt nomatch
setopt notify

## Setting Complement
autoload -Uz compinit; compinit
# show list of completions
setopt autolist
setopt automenu
setopt listpacked
setopt listtypes

bindkey -v
# End of lines configured by zsh-newuser-install
source "${ZDOTDIR:-$HOME}/.zprezto/init.zsh" 
